<?xml version="1.0" encoding="utf-8"?>
<process name="Sequential Assigner Test" key="SequentialAssignerTest" xmlns="http://jbpm.org/4.2/jpdl">

    <start name="Start">
        <transition to="New"/>
    </start>

    <custom name="New" class="workflow.activity.Assigner">
        <property name="role">
            <string value="Initiator"/>
        </property>
        <transition name="ToAgreement" to="Agreement"/>
    </custom>

    <custom name="Agreement" class="workflow.activity.SequentialAssigner">
        <property name="role">
            <string value="AgreementMember"/>
        </property>
        <property name="successTransition">
            <string value="Ok"/>
        </property>
        <transition name="Ok" to="Agreed"/>
        <transition name="NotOk" to="New"/>
    </custom>

    <custom name="Agreed" class="workflow.activity.CardActivity">
        <transition to="End"/>
    </custom>

    <end name="End"/>

</process>